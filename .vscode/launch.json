    "version": "0.2.0",
    "configurations": [
        {
            "name": "Listen for XDebug",
            "type": "php",
            "request": "launch",
            "port": 9000
        },
        {
            "name": "Launch currently open script",
            "type": "php",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "port": 9000
        },
        {
            "name": "Launch TEST MLMD",
            "stopOnEntry": false,
            "type": "php",
            "request": "launch",
            "program": "${workspaceFolder}/src/test.php",
            "cwd": "${workspaceFolder}",
            "env": { "XDEBUG_CONFIG": "idekey=VSCODE", "debug": "1", "XDEBUG_PROFILE": "1"},
            "port": 9000
        },
    ]
}